{% extends 'subLayout.html.twig' %}
{% block title%}
    Laporan Management DILo Malang
{% endblock %}
{% block content %}
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="card">

            <div class="body">
                <img src="http://malangfrontend.github.io/who-are-we/img/dilo-logo-default.png" class="print-only" width="20%"/>
                <h3>
                    LAPORAN MANAGEMENT DILo MALANG
                </h3>

                <h3>
                    <td> {% if data.manageMonth is defined and not null %}
                        {% if data.manageMonth == 1 %}
                            <span>Januari</span>
                        {% elseif data.manageMonth == 2 %}
                            <span>Februari</span>
                        {% elseif data.manageMonth == 3 %}
                            <span>Maret</span>
                        {% elseif data.manageMonth == 4 %}
                            <span>April</span>
                        {% elseif data.manageMonth == 5 %}
                            <span>Mei</span>
                        {% elseif data.manageMonth == 6 %}
                            <span>Juni</span>
                        {% elseif data.manageMonth == 7 %}
                            <span>Juli</span>
                        {% elseif data.manageMonth == 8 %}
                            <span>Agustus</span>
                        {% elseif data.manageMonth == 9 %}
                            <span>September</span>
                        {% elseif data.manageMonth == 10 %}
                            <span>Oktober</span>
                        {% elseif data.manageMonth == 11 %}
                            <span>November</span>
                        {% elseif data.manageMonth == 12 %}
                            <span>Desember</span>
                        {% endif %}
                    {% endif %}
                    </td>


                    <td> {% if data.manageYear is defined and not null %}
                            {% if data.manageYear == 1 %}
                                <span>2017</span>
                            {% elseif data.manageMonth == 2 %}
                                <span>2018</span>
                            {% elseif data.manageMonth == 3 %}
                                <span>2019</span>
                            {% elseif data.manageMonth == 4 %}
                                <span>2020</span>
                            {% elseif data.manageMonth == 5 %}
                                <span>2021</span>
                            {% elseif data.manageMonth == 6 %}
                                <span>2022</span>
                            {% elseif data.manageMonth == 7 %}
                                <span>2023</span>
                            {% elseif data.manageMonth == 8 %}
                                <span>2024</span>
                            {% elseif data.manageMonth == 9 %}
                                <span>2025</span>
                            {% elseif data.manageMonth == 10 %}
                                <span>...</span>

                            {% endif %}
                        {% endif %}
                    </td>

                </h3>


                <button class="no-print" type="button" onclick="print('printed-area')" class="btn btn-primary btn-xs" style="position:absolute;right:30px;border-radius: 0;"> CETAK</button>

                <div id="printed-area">
                    <table class="table table no-border table-striped table-hover dataTable js-exportable">
                        <thead>
                        <tr>
                            <th width="200">&nbsp;</th> {# Label #}
                            <th width="10">&nbsp;</th> {# TitikDua #}
                            <th width="">&nbsp;</th> {# Label #}
                            <th width="10">&nbsp;</th> {# TitikDua #}
                            <th width="200">&nbsp;</th> {# IsiAlamat #}

                        </tr>
                        </thead>
                        <tbody>

                        <tr>
                            <td class="fw-bold">Jumlah Member</td>
                            <td colspan="6"> : &nbsp;&nbsp; {% if data.manageDataMember is defined %}{{ data.manageDataMember }}{% else %}-{% endif %} &nbsp Orang </td>
                        </tr>

                        <tr>
                            <td class="fw-bold">Jumlah Pengunjung </td>
                            <td colspan="6"> : &nbsp;&nbsp; {% if data.manageDataUser is defined %}{{ data.manageDataUser }}{% else %}-{% endif %} &nbsp Orang </td>
                        </tr>
                        <tr>
                            <td class="fw-bold">Jumlah Follower Fanspage Facebook </td>
                            <td colspan="6"> : &nbsp; {% if data.manageDatafb is defined and not null %}{{ data.manageDatafb }}{% endif %} &nbsp Orang </td>
                        </tr>
                        <tr>
                            <td class="fw-bold">Jumlah Follower Instagram </td>
                            <td colspan="6"> : &nbsp; {% if data.manageDataig is defined and not null %}{{ data.manageDataig }}{% endif %} &nbsp Orang </td>
                        </tr>
                        <tr>
                            <td class="fw-bold">Jumlah Follower Twitter </td>
                            <td colspan="6"> : &nbsp; {% if data.manageDatatwitter is defined and not null %}{{ data.manageDatatwitter }}{% endif %} &nbsp Orang </td>
                        </tr>
                                               <tr>
                            <td class="fw-bold">Jumlah Startup </td>
                            <td colspan="6"> : &nbsp; {% if data.manageDatastartup is defined and not null %}{{ data.manageDatastartup }}{% endif %} &nbsp Startup </td>
                        </tr>
                        <tr>
                            <td class="fw-bold">Jumlah Komunitas </td>
                            <td colspan="6"> : &nbsp; {% if data.manageDatacommunity is defined and not null %}{{ data.manageDatacommunity }}{% endif %} &nbsp Komunitas </td>
                        </tr>

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>



{% endblock %}